#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int tw, adult, child;
    scanf("%d%d%d", &tw, &adult, &child) ;
   int w=(adult*75) +(child*30) ;
    if(w<tw) 
    {
        printf("Boat is stable ") ;
    }
    else{
        printf("Boat will drown") ;
    }
    return 0;
}
